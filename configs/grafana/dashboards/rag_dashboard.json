{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Request Rate (req/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(rag_requests_total[1m])",
          "legendFormat": "{{method}} {{endpoint}} {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" }
      }
    },
    {
      "title": "Query Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(rag_query_latency_seconds_bucket[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(rag_query_latency_seconds_bucket[5m]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(rag_query_latency_seconds_bucket[5m]))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Cache Hit Rate",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "rag_cache_hits_total / (rag_cache_hits_total + rag_cache_misses_total)",
          "legendFormat": "hit rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.3 },
              { "color": "green", "value": 0.6 }
            ]
          }
        }
      }
    },
    {
      "title": "Cache Hits vs Misses",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 8 },
      "targets": [
        {
          "expr": "rate(rag_cache_hits_total[1m])",
          "legendFormat": "hits"
        },
        {
          "expr": "rate(rag_cache_misses_total[1m])",
          "legendFormat": "misses"
        }
      ]
    },
    {
      "title": "GPU VRAM Usage (GB)",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "rag_gpu_vram_used_gb",
          "legendFormat": "VRAM"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decgbytes",
          "min": 0,
          "max": 6.4,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 4.0 },
              { "color": "red", "value": 5.5 }
            ]
          }
        }
      }
    },
    {
      "title": "Circuit Breaker Cost (EUR)",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 8 },
      "targets": [
        {
          "expr": "rag_circuit_breaker_cumulative_cost_eur",
          "legendFormat": "cost"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyEUR",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 3.0 },
              { "color": "red", "value": 4.5 }
            ]
          }
        }
      }
    },
    {
      "title": "Errors",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "rate(rag_errors_total[1m])",
          "legendFormat": "{{error_type}}"
        }
      ]
    },
    {
      "title": "Retrieval vs Generation Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(rag_retrieval_latency_seconds_bucket[5m]))",
          "legendFormat": "retrieval p95"
        },
        {
          "expr": "histogram_quantile(0.95, rate(rag_generation_latency_seconds_bucket[5m]))",
          "legendFormat": "generation p95"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["rag", "production"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Advanced RAG â€” Production Dashboard",
  "uid": "rag-production",
  "version": 1
}
